class Student_with_Attributes():
  def __init__ (Self, Name, StudentNo, Age, School, Course):
    Self.Name = Name
    Self.StudentNo = StudentNo
    Self.Age = Age
    Self.School = School
    Self.Course = Course

  def Ako(Self):
    return Self.Name + Self.StudentNo + Self.Age + Self.School + Self.Course
  
  def Section(Self):
    print(Self.Ako())

Ako = Student_with_Attributes("Jao, Jesus Mike B., ", "202114088, ", "20, ", "Adamson University, ", "BS Computer Engineering")
Ako.Section()
